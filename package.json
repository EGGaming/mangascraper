{
  "name": "@specify_/mangascraper",
  "version": "3.2.3",
  "description": "A mangascraper for fetching manga from a variety of manga sources (mangakakalot, manganato, and etc.)",
  "private": false,
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "build": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "scraper",
    "manga",
    "anime",
    "manhua",
    "manhwa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EGGaming/mangascraper.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/random-useragent": "^0.3.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "puppeteer": "*"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.22.1",
    "jquery": "^3.6.0",
    "random-useragent": "^0.5.0"
  }
}
